    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-async-handler": "1.2.0",
    "got": "13.0.0",
    "http-status-codes": "2.3.0",
    "inversify": "6.0.2",
    "jose": "5.1.0",
    "mime-types": "2.1.35",
    "mongoose": "7.6.3",
    "multer": "1.4.5-lts.1",
    "pino": "8.16.0",
    "reflect-metadata": "0.1.13"
