    "@types/express": "4.17.21",
    "@types/mime-types": "2.1.4",
    "@types/multer": "1.4.10",
    "@types/node": "20.11.18",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "eslint": "8.56.0",
    "eslint-config-htmlacademy": "9.1.1",
    "json-server": "0.17.4",
    "nodemon": "3.0.3",
    "pino-pretty": "10.2.3",
    "rimraf": "5.0.1",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
